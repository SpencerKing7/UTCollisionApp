@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <a asp-controller="Admin" asp-action="CrashTable" asp-route-county="" asp-route-pageNum="1" class="btn btn-block btn-outline-secondary">ALL CRASHES</a>

    @foreach (string c in Model)
    {
        <a asp-controller="Admin" asp-action="CrashTable" asp-route-county="@c" asp-route-pageNum="1"
           class="btn btn-block @(ViewBag.SelectedCategory == c ? "btn-dark" : "btn-outline-secondary")">@c</a>
    }
</div>
